<% layout("/layouts/boilerplate") %>
<div class="row mt-3">
  <h1 class="clo-6 offset-3">Signup on Wonderlust</h1>
    <div class="col-6 offset-3">
       <form action="/signup" method="POST" novalidate class="needs-validation" >
        <div class="mb-3">
            <lable for="username" class="form-lable">Username</lable>
            <input name="username"  
              class="form-control" 
              type="text" 
              required
            />
        <div class="valid-feedback">Look`s Good</div>
        </div>
        <div class="mb-3">
             <lable for="email" class="form-lable">Email</lable>
            <input name="email"  
              class="form-control" 
              type="text" 
              required
            />  
        </div>
        <div class="mb-3">
            <lable for="password" class="form-lable">Password</lable>
            <input name="password"  
            class="form-control" 
            type="password" 
            required
            />  
        </div>
          <button class="btn btn-success">SignUp</button>
       </form>
    </div>
</div>